{
    "name": "gravitatenz/fta-csp",
    "description": "csp helper/nonce generator thingamagiggy",
    "type": "library",
    "license": "no",
    "prefer-stable" : true,
    "minimum-stability" : "dev",
    "authors": [
        {
            "name": "John Lennard",
            "email": "john@yakmoo.se"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "symfony/http-foundation": ">=4.4",
        "twig/twig": ">=3.0",
        "symfony/event-dispatcher": ">=4.4",
        "symfony/http-kernel": ">=4.4",
        "lstrojny/functional-php": "^1.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^v6.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "GravitateNZ\\fta\\csp\\" : "src/"
          }
    },
    "autoload-dev": {
      "psr-4": {
        "GravitateNZ\\fta\\csp\\Tests\\": "tests/"
      }
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/gravitatenz/"},
        {"packagist.org": false}
    ],
"extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
