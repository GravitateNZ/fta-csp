{
    "name": "gravitatenz/fta-csp",
    "description": "csp helper/nonce generator thingamagiggy",
    "type": "library",
    "license": "BSD-3-Clause",
    "prefer-stable" : true,
    "minimum-stability" : "dev",
    "authors": [
        {
            "name": "John Lennard",
            "email": "john@yakmoo.se"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-foundation": "^5.4",
        "twig/twig": "^3.3",
        "symfony/event-dispatcher": "^5.4",
        "symfony/http-kernel": "^5.4",
        "lstrojny/functional-php": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/test-pack": "^1.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "GravitateNZ\\fta\\csp\\" : "src/"
          }
    },
    "autoload-dev": {
      "psr-4": {
        "GravitateNZ\\fta\\csp\\Tests\\": "tests/"
      }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
